{
  "ecmaVersion": 6,
  "libs": [
    "browser"
  ],
  "loadEagerly": [
    "ScreepsAutocomplete/**/*.js",
    "Src/**/**/*.js",
    "lodash/**/*.js"
  ],
  "plugins": {
    "modules": {
      "dontLoad": "",
      "load": "",
      "modules": ""
    },
    "doc_comment": {
      "fullDocs": true,
      "strong": false
    },
    "node_resolve": {},
    "es_modules": {},
    "complete_strings": {
      "maxLength": 15
    }
  }
}